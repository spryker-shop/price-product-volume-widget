{
  "name": "spryker-shop/price-product-volume-widget",
  "type": "library",
  "description": "PriceProductVolumeWidget module",
  "license": "proprietary",
  "require": {
    "php": ">=7.2",
    "spryker-shop/shop-ui": "^1.31.1",
    "spryker/currency": "^3.4.0",
    "spryker/kernel": "^3.30.0",
    "spryker/price": "^5.3.0",
    "spryker/price-product-storage": "^2.1.0 || ^4.0.0",
    "spryker/util-encoding": "^2.0.0"
  },
  "require-dev": {
    "spryker-shop/product-detail-page": "*",
    "spryker/code-sniffer": "*"
  },
  "suggest": {
    "spryker-shop/product-detail-page": "Widget plugins are available for this module."
  },
  "autoload": {
    "psr-4": {
      "SprykerShop\\": "src/SprykerShop/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/",
    "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
